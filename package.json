{
  "name": "oh-my-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma-generate-client:dev": "npx prisma generate --schema prisma/postgresql/schema.prisma",
    "prisma-generate-client:prod": "npx prisma generate --schema prisma/cockroachdb/schema.prisma",
    "migrate:dev": "npm run prisma-generate:dev && dotenv -e .env.development -- npx prisma migrate dev --schema prisma/postgresql/schema.prisma --name initial",
    "migrate:prod": "npm run prisma-generate:prod && dotenv -e .env.production -- npx prisma migrate dev --schema prisma/cockroachdb/schema.prisma --name initial"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@monaco-editor/react": "^4.6.0",
    "@prisma/client": "^5.18.0",
    "firebase": "^10.13.0",
    "firebase-admin": "^12.3.1",
    "framer-motion": "^11.3.28",
    "imagekit": "^5.1.0",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.3.0",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "prisma": "^5.18.0",
    "typescript": "^5"
  }
}
